<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="tree" nsURI="http://www.example.org/tree" nsPrefix="tree">
  <eClassifiers xsi:type="ecore:EClass" name="Root" eSuperTypes="#//Node"/>
  <eClassifiers xsi:type="ecore:EClass" name="PlainNode" eSuperTypes="#//Node">
    <eStructuralFeatures xsi:type="ecore:EReference" name="father" eType="#//Root"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dad" eType="#//PlainNode"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Node">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        defaultValueLiteral=""/>
  </eClassifiers>
</ecore:EPackage>
